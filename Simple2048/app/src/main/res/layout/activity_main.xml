<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rlMain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tv1"
        style="@style/TableItem" />

    <TextView
        android:id="@+id/tv2"
        style="@style/TableItem"
        android:layout_toRightOf="@+id/tv1" />

    <TextView
        android:id="@+id/tv3"
        style="@style/TableItem"
        android:layout_toRightOf="@+id/tv2"

        />

    <TextView
        android:id="@+id/tv4"
        style="@style/TableItem"
        android:layout_toRightOf="@+id/tv3" />

    <TextView
        android:id="@+id/tv5"
        style="@style/TableItem"
        android:layout_below="@+id/tv1" />

    <TextView
        android:id="@+id/tv6"
        style="@style/TableItem"
        android:layout_below="@+id/tv1"
        android:layout_toRightOf="@id/tv5" />

    <TextView
        android:id="@+id/tv7"
        style="@style/TableItem"
        android:layout_below="@+id/tv1"
        android:layout_toRightOf="@id/tv6" />

    <TextView
        android:id="@+id/tv8"
        style="@style/TableItem"
        android:layout_below="@+id/tv1"
        android:layout_toRightOf="@id/tv7" />

    <TextView
        android:id="@+id/tv9"
        style="@style/TableItem"
        android:layout_below="@+id/tv5" />

    <TextView
        android:id="@+id/tv10"
        style="@style/TableItem"
        android:layout_below="@+id/tv5"

        android:layout_toRightOf="@+id/tv9" />

    <TextView
        android:id="@+id/tv11"
        style="@style/TableItem"
        android:layout_below="@+id/tv5"

        android:layout_toRightOf="@+id/tv10" />

    <TextView
        android:id="@+id/tv12"
        style="@style/TableItem"
        android:layout_below="@+id/tv5"

        android:layout_toRightOf="@+id/tv11" />

    <TextView
        android:id="@+id/tv13"
        style="@style/TableItem"
        android:layout_below="@+id/tv9" />

    <TextView
        android:id="@+id/tv14"
        style="@style/TableItem"
        android:layout_below="@+id/tv9"

        android:layout_toRightOf="@+id/tv13" />

    <TextView
        android:id="@+id/tv15"
        style="@style/TableItem"
        android:layout_below="@+id/tv9"

        android:layout_toRightOf="@+id/tv14" />

    <TextView
        android:id="@+id/tv16"
        style="@style/TableItem"
        android:layout_below="@+id/tv9"
        android:layout_toRightOf="@+id/tv15" />

    <LinearLayout
        android:id="@+id/llResult"
        style="@style/llResult"
        android:layout_alignParentBottom="true">

        <TextView
            style="@style/results"
            android:text="@string/Result" />

        <TextView
            android:id="@+id/tvResult"
            style="@style/results"
            android:text="0" />
    </LinearLayout>


</RelativeLayout>
